{
  "cli": {
    "version": ">= 0.47.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "image": "latest"
      }
    },
    "production": {
      "channel": "production",
      "android": {
        "image": "ubuntu-20.04-jdk-11-ndk-r21e"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
